    "mocha": "^5.2.0",
    "postcss": "^7.0.5",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.1.1",
    "postcss-reporter": "^6.0.0",
    "postcss-url": "^8.0.0",
    "prettier": "^1.14.3",
