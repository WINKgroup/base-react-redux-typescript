    "@types/history": "4.7.1",
    "@types/mocha": "^5.2.5",
    "@types/node": "10.11.7",
    "@types/react": "16.4.16",
    "@types/react-dom": "16.0.9",
    "@types/react-redux": "6.0.9",
    "@types/react-router": "4.0.32",
    "@types/redux-actions": "2.3.1",
    "@types/sinon": "^7.0.0",
    "@types/sinon-chai": "^3.2.2",
    "@types/storybook__react": "^4.0.0",
    "@types/webpack": "4.4.16",
    "@types/webpack-env": "1.13.6",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "css-loader": "^1.0.0",
