{
  "name": "ubique-digital-invoice",
  "version": "1.0.0",
  "private": true,
  "description": "A frontend boilerplate with React, Redux and Typescript",
  "main": "index.js",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "NODE_PATH=./src ./node_modules/.bin/mocha -r ./testsConfig.js ./**/*.spec.ts",
    "test:watch": "yarn test --watch-extensions ts --watch",
    "start": "webpack-dev-server --mode development --hot --progress --color --port 3000 --open",
    "build": "webpack -p --progress --colors",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@storybook/react": "^4.1.2",
